<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:BudgetApp_WPF.MVVM.Views"
                    xmlns:viewModels="clr-namespace:BudgetApp_WPF.MVVM.ViewModels"
                    
                    xmlns:incomeView="clr-namespace:BudgetApp_WPF.MVVM.Views.Income"
                    xmlns:incomeViewModel="clr-namespace:BudgetApp_WPF.MVVM.ViewModels.Income"
                    
                    xmlns:expenseView="clr-namespace:BudgetApp_WPF.MVVM.Views.Expense"
                    xmlns:expenseViewModel="clr-namespace:BudgetApp_WPF.MVVM.ViewModels.Expense">
    <DataTemplate DataType="{x:Type viewModels:CurrentMonthViewModel}">
        <views:CurrentMonthView/>
    </DataTemplate>




    <DataTemplate DataType="{x:Type expenseViewModel:ExpenseDashBoardViewModel}">
        <expenseView:ExpenseDashBoardView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type incomeViewModel:IncomeDashBoardViewModel}">
        <incomeView:IncomeDashBoardView/>
    </DataTemplate>
    
    
</ResourceDictionary>